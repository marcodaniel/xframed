\cs_new:Npn \__xframed__show_length:n #1
 {
  \dim_set:Nn \l_tmpa_dim { \__xframed__get_length:n { #1 } }
  \dim_use:N \l_tmpa_dim
 }
\cs_new:Npn \__xframed__show_skip:n #1
